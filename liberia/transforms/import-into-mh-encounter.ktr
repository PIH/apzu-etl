    <connection>OpenMRS</connection>
    <sql>/*
	Extracts all encounters
*/
SELECT e.encounter_id, e.patient_id, e.encounter_datetime, e.visit_id, mh.* 
FROM encounter e 
JOIN (
SELECT o.encounter_id, 
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'Role of referring person' AND cn.name = 'Teacher' THEN TRUE END) AS `referred_by_teacher`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'Role of referring person' AND cn.name = 'TRADITIONAL HEALER' THEN TRUE END) AS `referred_by_traditional_healer`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'Role of referring person' AND cn.name = 'VILLAGE HEALTH WORKER' THEN TRUE END) AS `referred_by_vhw`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'Role of referring person' AND cn.name = 'Religious institution' THEN TRUE END) AS `referred_by_religious_institution`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'Role of referring person' AND cn.name = 'Self-refer' THEN TRUE END) AS `referred_by_self`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'Role of referring person' AND cn.name = 'Other medical service' THEN TRUE END) AS `referred_by_oms`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'Type of referral role' AND cn.name = 'DOCTOR' THEN TRUE END) AS `referral_role_doctor`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'Type of referral role' AND cn.name = 'NURSE' THEN TRUE END) AS `referral_role_nurse`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'Type of referral role' AND cn.name = 'SOCIAL WORKER SERVICE' THEN TRUE END) AS `referral_role_sws`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'PATIENT HOSPITALIZED SINCE LAST VISIT' THEN cn.name END) AS `hospitalized_since_last_visit`,
MAX(CASE WHEN crs.name = 'CIEL' AND crt.code = '165137' THEN o.value_numeric END) AS `phq_9`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'PATIENT PREGNANT' THEN o.value_coded END) AS `pregnant`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'HISTORY OF ALCOHOL USE' THEN cn.name END) AS `history_alcohol_use`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'HISTORY OF ILLEGAL DRUGS' THEN cn.name END) AS `history_drug_use`,

MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'Mental health intervention' AND cn.name = 'Supportive psychotherapy' THEN TRUE END) AS `intervention_supportive_psychotherapy`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'Mental health intervention' AND cn.name = 'Psychotherapy' THEN TRUE END) AS `intervention_psychotherapy`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'Mental health intervention' AND cn.name = 'Psychoeducation' THEN TRUE END) AS `intervention_psychoeducation`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'Mental health intervention' AND cn.name = 'Relaxation' THEN TRUE END) AS `intervention_relaxation`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'Mental health intervention' AND cn.name = 'Family Support' THEN TRUE END) AS `intervention_family_support`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'Mental health intervention' AND cn.name = 'Grief counseling' THEN TRUE END) AS `intervention_grief_counseling`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'Mental health intervention' AND cn.name = 'Parenting skills' THEN TRUE END) AS `intervention_parenting_skills`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'Mental health intervention' AND cn.name = 'Behavioral activation' THEN TRUE END) AS `intervention_behavioral_activation`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'Mental health intervention' AND cn.name = 'Security plan' THEN TRUE END) AS `intervention_security_plan`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'Mental health intervention' AND cn.name = 'PSYCHOSOCIAL COUNSELING' THEN TRUE END) AS `intervention_psychosocial_counseling`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'Mental health intervention' AND cn.name = 'HIV/AIDS counseling' THEN TRUE END) AS `intervention_hiv_counseling`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'Mental health intervention' AND cn.name = 'Informing patient of diagnosis' THEN TRUE END) AS `intervention_ipod`,

MAX(CASE WHEN crs.name = 'PIH' AND crt.code = '2156' AND cn.name = 'Financial aid' THEN TRUE END) AS `sea_received_financial_aid`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = '2157' AND cn.name = 'Financial aid' THEN TRUE END) AS `sea_recommended_financial_aid`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = '2156' AND cn.name = 'Transport assistance' THEN TRUE END) AS `sea_received_transport_assistance`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = '2157' AND cn.name = 'Transport assistance' THEN TRUE END) AS `sea_recommended_transport_assistance`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = '2156' AND cn.name = 'Nutritional assistance' THEN TRUE END) AS `sea_received_nutritional_assistance`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = '2157' AND cn.name = 'Nutritional assistance' THEN TRUE END) AS `sea_recommended_nutritional_assistance`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = '2156' AND cn.name = 'School assistance' THEN TRUE END) AS `sea_received_school_assistance`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = '2157' AND cn.name = 'School assistance' THEN TRUE END) AS `sea_recommended_school_assistance`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = '2156' AND cn.name = 'Housing assistance' THEN TRUE END) AS `sea_received_housing_assistance`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = '2157' AND cn.name = 'Housing assistance' THEN TRUE END) AS `sea_recommended_housing_assistance`,
MAX(CASE WHEN crs.name = 'PIH' AND crt.code = 'RETURN VISIT DATE' THEN o.value_datetime END) AS `return_visit_date`
FROM obs o JOIN encounter e USING (encounter_id) 
JOIN encounter_type et ON e.encounter_type=et.encounter_type_id 
JOIN concept c ON o.concept_id = c.concept_id
JOIN concept_reference_map crm ON c.concept_id = crm.concept_id
JOIN concept_reference_term crt USING(concept_reference_term_id)
JOIN concept_reference_source crs USING (concept_source_id)
LEFT JOIN concept_name cn ON o.value_coded = cn.concept_id AND cn.locale = 'en' AND cn.locale_preferred = '1'  AND cn.voided = 0
WHERE et.uuid = 'a8584ab8-cc2a-11e5-9956-625662870761'
GROUP BY o.encounter_id
) mh ON e.encounter_id=mh.encounter_id </sql>
