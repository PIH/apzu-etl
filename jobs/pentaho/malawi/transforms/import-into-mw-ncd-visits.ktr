    <sql>SELECT e.patient_id, &#xa;e.encounter_date as visit_date,&#xa;e.location,&#xa;CASE WHEN &#x28;depressive &#x2b; hypomania &#x2b; disrupt &#x2b; abnormal &#x2b; delusion &#x2b; hallucinate &#x2b; lackInsight &#x2b; other&#x29; &#x3d; 8 THEN &#x27;Normal&#x27; end as mental_status_exam&#xa;from omrs_encounter e &#xa;LEFT JOIN &#x28;SELECT encounter_id, 1 AS depressive from omrs_obs where concept &#x3d; &#x27;Mental health chief complaint absent&#x27; AND value_coded &#x3d; &#x22;Depressive Disorder&#x22;&#x29; depressive ON depressive.encounter_id &#x3d; e.encounter_id&#xa;LEFT JOIN &#x28;SELECT encounter_id, 1 AS hypomania from omrs_obs where concept &#x3d; &#x27;Mental health chief complaint absent&#x27; AND value_coded &#x3d; &#x22;Hypomania&#x22;&#x29; hypomania ON hypomania.encounter_id &#x3d; e.encounter_id&#xa;LEFT JOIN &#x28;SELECT encounter_id, 1 AS disrupt from omrs_obs where concept &#x3d; &#x27;Mental health chief complaint absent&#x27; AND value_coded &#x3d; &#x22;Disruptive Behavior Disorder&#x22;&#x29; disrupt ON disrupt.encounter_id &#x3d; e.encounter_id&#xa;LEFT JOIN &#x28;SELECT encounter_id, 1 AS abnormal from omrs_obs where concept &#x3d; &#x27;Mental health chief complaint absent&#x27; AND value_coded &#x3d; &#x22;Abnormal speech&#x22;&#x29; abnormal ON abnormal.encounter_id &#x3d; e.encounter_id&#xa;LEFT JOIN &#x28;SELECT encounter_id, 1 AS delusion from omrs_obs where concept &#x3d; &#x27;Mental health chief complaint absent&#x27; AND value_coded &#x3d; &#x22;Delusions&#x22;&#x29; delusion ON delusion.encounter_id &#x3d; e.encounter_id&#xa;LEFT JOIN &#x28;SELECT encounter_id, 1 AS hallucinate from omrs_obs where concept &#x3d; &#x27;Mental health chief complaint absent&#x27; AND value_coded &#x3d; &#x22;Hallucinations&#x22;&#x29; hallucinate ON hallucinate.encounter_id &#x3d; e.encounter_id&#xa;LEFT JOIN &#x28;SELECT encounter_id, 1 AS lackInsight from omrs_obs where concept &#x3d; &#x27;Mental health chief complaint absent&#x27; AND value_coded &#x3d; &#x22;Lack of insight&#x22;&#x29; lackInsight ON lackInsight.encounter_id &#x3d; e.encounter_id&#xa;LEFT JOIN &#x28;SELECT encounter_id, 1 AS other from omrs_obs where concept &#x3d; &#x27;Mental health chief complaint absent&#x27; AND value_coded &#x3d; &#x22;Other non-coded&#x22;&#x29; other ON other.encounter_id &#x3d; e.encounter_id&#xa;WHERE e.encounter_type &#x3d; &#x27;MENTAL_HEALTH_FOLLOWUP&#x27;&#xa;group by 	e.patient_id, e.encounter_date, e.location&#xa;order by 	patient_id, visit_date, location&#x3b;</sql>
