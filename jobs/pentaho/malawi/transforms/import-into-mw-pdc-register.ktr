    <sql>/*
	Extract superset of all PDC program enrollments
*/
select      p.program_enrollment_id,
			p.patient_id,
			p.location,
			p.enrollment_date as start_date,
			p.completion_date as end_date
from 		omrs_program_enrollment p
where 		p.program = 'Pediatric Development Clinic Program'
order by 	p.program_enrollment_id, patient_id, location</sql>
