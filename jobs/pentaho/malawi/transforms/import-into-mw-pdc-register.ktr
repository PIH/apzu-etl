    <connection>Warehouse</connection>
    <sql>/*
	Extract superset of all PDC States
*/
select      p.program_enrollment_id, s.state as status, if(p.completion_date is null, null, ifnull(s.state, p.outcome)) as outcome
from 		omrs_program_state s
inner join	omrs_program_enrollment p on p.program_enrollment_id = s.program_enrollment_id
where 		s.program_state_id = (
	          select        s1.program_state_id
              from          omrs_program_state s1
			  where			s1.program_enrollment_id = s.program_enrollment_id
              and           s1.program = 'Pediatric Development Clinic Program'
			  and			s1.workflow = 'Chronic care treatment status'
              order by      s1.start_date desc limit 1

)
order by 	p.program_enrollment_id, p.patient_id, p.location</sql>
